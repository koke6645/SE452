<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.depaul.cdm.bank.form">
  <class name="Credit" table="CREDITS">
    <id column="CREDIT_ID" name="creditId" type="integer">
      <generator class="native"/>
    </id>
    <property column="CREDIT_TYPE" name="creditType"/>
    <property column="ACCOUNT_ID_FK" name="accountId"/>
    <property column="AMOUNT" name="amount"/>
    <property column="POSTED_DATE" name="postedDate" type="date"/>
    <property column="CATEGORY" name="category"/>
    <property column="BALANCE" name="balance"/>
    <property column="DESCRIPTION" name="description"/>
  </class>
</hibernate-mapping>
