<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.depaul.cdm.bank.form">
  <class name="Account" table="ACCOUNT">
    <id column="ACCOUNT_ID" name="accountId" type="integer">
      <generator class="native"/>
    </id>
    <property column="ACCOUNT_TYPE" name="accountType" not-null="true"/>
    <property column="BALANCE" name="balance" not-null="true" type="double"/>
    <property column="INTEREST_RATE" name="interestRate" not-null="false" type="double"/>
    <property column="User_ID" name="userId" not-null="true" type="integer"/>
  </class>
</hibernate-mapping>
