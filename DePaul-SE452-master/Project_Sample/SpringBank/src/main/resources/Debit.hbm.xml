<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.depaul.cdm.bank.form">
  <class name="Debit" table="DEBITS">
    <id column="DEBIT_ID" name="debitId">
      <generator class="native"/>
    </id>
    <property column="ACCOUNT_ID_FK" name="accountId"/>
    <property column="AMOUNT" name="amount"/>
    <property column="POSTED_DATE" name="postedDate" type="date"/>
    <property column="DEBIT_TYPE" name="debitType"/>
    <property column="BALANCE" name="balance"/>
  </class>
</hibernate-mapping>
